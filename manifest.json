{

    "manifest_version": 2,
    "name": "RainbowTables",
    "version": "0.0.1",
    "action":{},
    "description": "Makes any table pretty with one click",
  
    "browser_action": {
        "default_icon": {
          "64":"img/rainbow64.png"
        }
    },
  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["main.js"]
      }
    ],
    "background": {
        "scripts":["background.js"]
    },
    "permissions":["activeTab"]
}
